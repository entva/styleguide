{
  "name": "eslint-config-entva-next",
  "description": "Shareable typescript styleguide config for Next.js",
  "author": "Max Degterev <max@degterev.me>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/entva/styleguide"
  },
  "bugs": "https://github.com/entva/styleguide/issues",
  "version": "2.20.0",
  "keywords": [
    "linter",
    "config",
    "eslint",
    "typescript"
  ],
  "type": "module",
  "main": "index.js",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "test": "node ../../../test ./test js tsx",
    "rules": "node_modules/.bin/eslint --print-config file.tsx > eslint-ruleset-output.json",
    "prepublishOnly": "npm test"
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.21.0",
    "next": "^15.x.x",
    "react": "^19.x.x",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint-config-entva-typescript": "^2.19.0",
    "eslint-config-next": "^15.2.0"
  },
  "peerDependencies": {
    "eslint": "^9.x.x",
    "next": "^14.x.x || ^15.x.x"
  }
}
