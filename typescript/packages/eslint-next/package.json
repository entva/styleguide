{
  "name": "eslint-config-entva-next",
  "description": "Shareable typescript styleguide config for Next.js",
  "author": "Max Degterev <max@degterev.me>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/entva/styleguide.git"
  },
  "bugs": "https://github.com/entva/styleguide/issues",
  "version": "2.32.0",
  "keywords": [
    "linter",
    "config",
    "eslint",
    "typescript"
  ],
  "type": "module",
  "main": "index.js",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "test": "node ../../../test ./test js tsx",
    "rules": "eslint --print-config file.tsx > eslint-ruleset-output.json",
    "prepublishOnly": "npm test"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9.38.0",
    "next": "^16.x.x",
    "react": "^19.x.x",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint-config-entva-typescript": "^2.x.x",
    "eslint-config-next": "^15.5.6"
  },
  "peerDependencies": {
    "eslint": "^9.x.x",
    "next": "^15.x.x || ^16.x.x"
  }
}
