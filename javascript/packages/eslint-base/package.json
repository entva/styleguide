{
  "name": "eslint-config-entva-base",
  "description": "Shareable entva styleguide config for eslint",
  "author": "Max Degterev <max@degterev.me>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/entva/styleguide.git"
  },
  "bugs": "https://github.com/entva/styleguide/issues",
  "version": "2.30.0",
  "keywords": [
    "linter",
    "config",
    "eslint"
  ],
  "type": "module",
  "main": "index.js",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "test": "node ../../../test ./test js",
    "rules": "eslint --print-config file.js > eslint-ruleset-output.json",
    "prepublishOnly": "npm test"
  },
  "devDependencies": {
    "eslint": "^9.32.0"
  },
  "dependencies": {
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.3.0"
  },
  "peerDependencies": {
    "eslint": "^9.x.x"
  }
}
